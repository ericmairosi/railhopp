# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Darwin Pub/Sub (via Broker)
DARWIN_ENABLED=true
DARWIN_BROKER_URL=http://localhost:4001
DARWIN_USERNAME=your_darwin_username
DARWIN_PASSWORD=your_darwin_password

# Network Rail Credentials
NETWORK_RAIL_USERNAME=your_network_rail_username
NETWORK_RAIL_PASSWORD=your_network_rail_password

# Redis Configuration (Upstash)
REDIS_URL=your_redis_url
REDIS_TOKEN=your_redis_token

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3001

# Analytics & Monitoring
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
SENTRY_DSN=your_sentry_dsn

# Map API Keys
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Environment
NODE_ENV=development

# Enhanced Multi-API Configuration for Comprehensive UK Rail Data

# Network Rail APIs (Train Movements, VSTP, SMART, CORPUS, etc.)
NETWORK_RAIL_USERNAME=your_network_rail_username
NETWORK_RAIL_PASSWORD=your_network_rail_password
NETWORK_RAIL_API_URL=https://publicdatafeeds.networkrail.co.uk
NETWORK_RAIL_STOMP_URL=stomp://datafeeds.networkrail.co.uk:61618
NETWORK_RAIL_ENABLED=true

# National Rail APIs (Disruptions, Service Performance)
NATIONAL_RAIL_API_KEY=your_national_rail_api_key
NATIONAL_RAIL_API_URL=https://api.nationalrail.co.uk/api
NATIONAL_RAIL_ENABLED=true

# Enhanced Darwin Configuration
DARWIN_PUSH_PORT_ENABLED=false
DARWIN_PUSH_PORT_TOKEN=your_push_port_token

# Real-time Features
WEBSOCKET_ENABLED=true
WEBSOCKET_PORT=3001
REDIS_URL=redis://localhost:6379
REDIS_ENABLED=false

# Performance and Monitoring
API_RATE_LIMITING=true
API_CACHE_ENABLED=true
API_CACHE_TTL=30
MONITORING_ENABLED=true
